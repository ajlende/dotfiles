{
	"workbench.startupEditor": "newUntitledFile",

	"editor.fontFamily": "'Dank Mono', 'Fira Code', 'Noto Sans Mono', 'Noto Color Emoji', 'EmojiOne', monospace",
	"editor.fontLigatures": true,
	"editor.minimap.maxColumn": 100,
	"editor.rulers": [80, 100, 120],

	"files.insertFinalNewline": true,
	"files.associations": {
		"*.rasi": "less",
		".env": "shellscript",
		".env.example": "shellscript",
		"rush.json": "jsonc"
	},

	"color-highlight.markerType": "dot-before",

	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},

	"prettier.useTabs": true,

	"vim.incsearch": true,
	"vim.easymotion": true,
	"vim.useSystemClipboard": true,
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["<space>"],
			"commands": ["vspacecode.space"]
		}
	],
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": ["<space>"],
			"commands": ["vspacecode.space"]
		}
	],
	"vspacecode.bindingOverrides": [
		{
			"keys": "b.h",
			"name": "Focus left editor group",
			"type": "command",
			"command": "workbench.action.navigateLeft"
		},
		{
			"keys": "b.j",
			"name": "Focus below editor group",
			"type": "command",
			"command": "workbench.action.navigateDown"
		},
		{
			"keys": "b.k",
			"name": "Focus above editor group",
			"type": "command",
			"command": "workbench.action.navigateUp"
		},
		{
			"keys": "b.l",
			"name": "Focus right editor group",
			"type": "command",
			"command": "workbench.action.navigateRight"
		}
	],
	"javascript.updateImportsOnFileMove.enabled": "always",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"workbench.iconTheme": "Monokai Pro Icons",
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"security.workspace.trust.untrustedFiles": "open",
	"[markdown]": {
		"files.trimTrailingWhitespace": false,
		"editor.wordWrapColumn": 80,
		"editor.wordWrap": "on",
		"editor.quickSuggestions": false,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"cSpell.userWords": [
		"Overridable",
		"Overrideable",
		"Preprocessors",
		"repurpose",
		"stylesheet"
	],
	"markdown-preview-enhanced.previewTheme": "github-light.css",
	"markdown.extension.italic.indicator": "_",
	"zenMode.fullScreen": false,
	"gitlens.advanced.fileHistoryFollowsRenames": true,
	"window.zoomLevel": 1,
	"workbench.colorTheme": "Ayu Dark",
	"workbench.editorAssociations": {
		"git-rebase-todo": "default"
	},
	"editor.formatOnSave": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	}
}
